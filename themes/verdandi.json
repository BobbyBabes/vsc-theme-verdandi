{
	"$schema": "vscode://schemas/color-theme",
	"name": "Railgun",
	"palette": {
		"gray": [
			"#f8f9fa",
			"#e9ebee",
			"#dadee2",
			"#cbd0d5",
			"#bcc3c9",
			"#adb5bd",
			"#91989f",
			"#757b82",
			"#595f64",
			"#3d4247",
			"#212529"
		],
		"red": [
			"#fff5f5",
			"#ffd9d9",
			"#ffbebe",
			"#ffa2a2",
			"#ff8787",
			"#ff6b6b",
			"#f45e5e",
			"#e95151",
			"#df4444",
			"#d43737",
			"#c92a2a"
		],
		"green": [
			"#ebfbee",
			"#ccf2d3",
			"#ade9b8",
			"#8fe19c",
			"#70d881",
			"#51cf66",
			"#49c15e",
			"#42b356",
			"#3aa64e",
			"#339846",
			"#2b8a3e"
		],
		"blue": [
			"#e8f7ff",
			"#c4e4fc",
			"#9fd2f9",
			"#7bbff6",
			"#56adf3",
			"#329af0",
			"#2d8fe2",
			"#2884d4",
			"#2278c7",
			"#1d6db9",
			"#1862ab"
		]
	},
	"colors": {
		"focusBorder": "#329af0",
		"foreground": "#595f64",
		"errorForeground": "#f45e5e",
		"editor.background": "#e9ebee",
		"editor.foreground": "#595f64",
		"editor.lineHighlightBackground": "#dadee2",
		"editorCursor.foreground": "#329af0",
		"editorLineNumber.foreground": "#adb5bd",
		"editor.selectionBackground": "#9fd2f9ff",
		"editor.selectionHighlightBackground": "#c4e4fcff",
		"editor.inactiveSelectionBackground": "#cbd0d5ff",
		"debugToolBar.background": "#dadee2ff",
		"editorWidget.background": "#dadee2ff",
		"editorSuggestWidget.background": "#e9ebeeff",
		"editorGroup.border": "#cbd0d5",
		"editorGroupHeader.noTabsBackground": "#e9ebee",
		"editorGroupHeader.tabsBackground": "#dadee2",
		"tab.border": "#cbd0d5",
		"tab.inactiveBackground": "#dadee2",
		"tab.inactiveForeground": "#757b82",
		"tab.activeBackground": "#e9ebee",
		"tab.activeForeground": "#212529",
		"peekViewEditor.background": "#cbd0d555",
		"peekViewTitle.background": "#f8f9faff",
		"peekView.border": "#cbd0d5",
		"scrollbar.shadow": "#0000001a",
		"scrollbarSlider.background": "#00000013",
		"scrollbarSlider.activeBackground": "#00000026",
		"scrollbarSlider.hoverBackground": "#00000026",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#329af0",
		"editorGutter.addedBackground": "#51cf66",
		"editorGutter.deletedBackground": "#ff6b6b",
		"diffEditor.removedTextBackground": "#ff6b6b26",
		"diffEditor.insertedTextBackground": "#51cf661a",
		"sideBarTitle.foreground": "#212529",
		"sideBar.background": "#dadee2",
		"sideBar.border": "#cbd0d5",
		"sideBarSectionHeader.background": "#cbd0d5",
		"list.highlightForeground": "#2d8fe2",
		"list.hoverBackground": "#cbd0d5",
		"list.inactiveSelectionBackground": "#adb5bd4d",
		"list.activeSelectionBackground": "#329af040",
		"list.focusBackground": "#329af040",
		"list.inactiveSelectionForeground": "#212529",
		"list.activeSelectionForeground": "#212529",
		"list.focusForeground": "#212529",
		"dropdown.background": "#f8f9fa",
		"dropdown.border": "#cbd0d5",
		"dropdown.foreground": "#595f64",
		"input.background": "#f8f9fa",
		"input.border": "#cbd0d5",
		"input.foreground": "#595f64",
		"input.placeholderForeground": "#adb5bd",
		"button.background": "#2d8fe2",
		"button.foreground": "#e8f7ff",
		"button.hoverBackground": "#2884d4",
		"badge.background": "#329af0",
		"activityBar.background": "#bcc3c9",
		"activityBar.foreground": "#3d4247",
		"statusBar.background": "#cbd0d5",
		"statusBar.foreground": "#595f64",
		"statusBar.noFolderBackground": "#cbd0d5",
		"statusBar.noFolderForeground": "#595f64",
		"statusBar.debuggingBackground": "#cbd0d5",
		"statusBar.debuggingForeground": "#595f64",
		"panel.border": "#cbd0d5",
		"panelTitle.activeBorder": "#329af0"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment, punctuation.comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#91989f"
			}
		},
		{
			"name": "Operator",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212529"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation, delimiter, bracket, brace, paren, delimiter.tag, punctuation.tag, tag.html, tag.xml, meta.property-value punctuation.separator.key-value,punctuation.definition.metadata.md, string.link.md, meta.brace",
			"settings": {
				"foreground": "#757b82"
			}
		},
		{
			"name": "String",
			"scope": "string, meta.property-value.string, support.constant.property-value.string, meta.structure.dictionary.value.json string.quoted.double.json, meta.structure.dictionary.json string.quoted.double.json, meta.preprocessor string",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#757b82"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": "constant.numeric, meta.property-value.numeric, support.constant.property-value.numeric, meta.property-value.color, support.constant.property-value.color,constant.language",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "User names",
			"scope": "constant.character, constant.other,entity.name.function, entity.name.class, entity.other.inherited-class, entity.other.attribute-name, entity.name, entity.other.attribute-name, entity.other.attribute-name.html, support.type.property-name, entity.name.tag.table, meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, meta.property-value.keyword, support.constant.property-value.keyword, meta.preprocessor.keyword,keyword.other.use, keyword.other.function.use, keyword.other.namespace, keyword.other.new, keyword.other.special-method, keyword.other.unit, keyword.other.use-as",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212529"
			}
		},
		{
			"name": "Storage",
			"scope": "storage, storage.type, storage.type.ts, storage.type.var.ts, storage.type.js, storage.type.var.js, storage.type.const.ts, storage.type.let.ts, storage.type.let.js, storage.type.const.js, entity.name.tag",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212529"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": "meta.ptr, meta.pointer, meta.address, meta.array.cxx",
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "Library",
			"scope": "support.type, support.class, support.function, support.constant",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#e95151"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#e95151"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": "punctuation.definition.heading.md, entity.name.type.md, beginning.punctuation",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "Markdown titles",
			"scope": "markup.heading, entity.name.section",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212529"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": "markup.raw, markup.inline.raw, markup.fenced, markup.fenced_code",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#757b82"
			}
		},
		{
			"name": "Markdown link",
			"scope": "markup.link, string.other.link.title, string.other.link.description, meta.link.inline, meta.image.inline",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": "variable.language.makefile, variable.other.makefile",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": "entity.name.tag.css",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212529"
			}
		},
		{
			"name": "CSS Property",
			"scope": "meta.property-name.css",
			"settings": {
				"foreground": "#212529"
			}
		}
	]
}