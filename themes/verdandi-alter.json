{
	"$schema": "vscode://schemas/color-theme",
	"name": "Railgun",
	"palette": {
		"gray": [
			"#050607",
			"#111416",
			"#1d2125",
			"#2a2f34",
			"#363c43",
			"#424a52",
			"#60676e",
			"#7d848a",
			"#9ba0a6",
			"#b8bdc2",
			"#d6dade"
		],
		"red": [
			"#0a0000",
			"#260000",
			"#410000",
			"#5d0000",
			"#780000",
			"#940000",
			"#a10b0b",
			"#ae1616",
			"#bb2020",
			"#c82b2b",
			"#d53636"
		],
		"green": [
			"#041407",
			"#0d3313",
			"#165220",
			"#1e702c",
			"#278f39",
			"#30ae45",
			"#3eb652",
			"#4cbd60",
			"#59c56d",
			"#67cc7b",
			"#75d488"
		],
		"blue": [
			"#000f17",
			"#03243b",
			"#063960",
			"#094d84",
			"#0c62a9",
			"#0f77cd",
			"#1d7fd2",
			"#2b87d7",
			"#388edd",
			"#4696e2",
			"#549ee7"
		]
	},
	"colors": {
		"focusBorder": "#0f77cd",
		"foreground": "#9ba0a6",
		"errorForeground": "#a10b0b",
		"editor.background": "#111416",
		"editor.foreground": "#9ba0a6",
		"editor.lineHighlightBackground": "#1d2125",
		"editorCursor.foreground": "#0f77cd",
		"editorLineNumber.foreground": "#424a52",
		"editor.selectionBackground": "#063960ff",
		"editor.selectionHighlightBackground": "#03243bff",
		"editor.inactiveSelectionBackground": "#2a2f34ff",
		"debugToolBar.background": "#1d2125ff",
		"editorWidget.background": "#1d2125ff",
		"editorSuggestWidget.background": "#111416ff",
		"editorGroup.border": "#2a2f34",
		"editorGroupHeader.noTabsBackground": "#111416",
		"editorGroupHeader.tabsBackground": "#1d2125",
		"tab.border": "#2a2f34",
		"tab.inactiveBackground": "#1d2125",
		"tab.inactiveForeground": "#7d848a",
		"tab.activeBackground": "#111416",
		"tab.activeForeground": "#d6dade",
		"peekViewEditor.background": "#2a2f3455",
		"peekViewTitle.background": "#050607ff",
		"peekView.border": "#2a2f34",
		"scrollbar.shadow": "#0000001a",
		"scrollbarSlider.background": "#00000013",
		"scrollbarSlider.activeBackground": "#00000026",
		"scrollbarSlider.hoverBackground": "#00000026",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#0f77cd",
		"editorGutter.addedBackground": "#30ae45",
		"editorGutter.deletedBackground": "#940000",
		"diffEditor.removedTextBackground": "#94000026",
		"diffEditor.insertedTextBackground": "#30ae451a",
		"sideBarTitle.foreground": "#d6dade",
		"sideBar.background": "#1d2125",
		"sideBar.border": "#2a2f34",
		"sideBarSectionHeader.background": "#2a2f34",
		"list.highlightForeground": "#1d7fd2",
		"list.hoverBackground": "#2a2f34",
		"list.inactiveSelectionBackground": "#424a524d",
		"list.activeSelectionBackground": "#0f77cd40",
		"list.focusBackground": "#0f77cd40",
		"list.inactiveSelectionForeground": "#d6dade",
		"list.activeSelectionForeground": "#d6dade",
		"list.focusForeground": "#d6dade",
		"dropdown.background": "#050607",
		"dropdown.border": "#2a2f34",
		"dropdown.foreground": "#9ba0a6",
		"input.background": "#050607",
		"input.border": "#2a2f34",
		"input.foreground": "#9ba0a6",
		"input.placeholderForeground": "#424a52",
		"button.background": "#1d7fd2",
		"button.foreground": "#000f17",
		"button.hoverBackground": "#2b87d7",
		"badge.background": "#0f77cd",
		"activityBar.background": "#363c43",
		"activityBar.foreground": "#b8bdc2",
		"statusBar.background": "#2a2f34",
		"statusBar.foreground": "#9ba0a6",
		"statusBar.noFolderBackground": "#2a2f34",
		"statusBar.noFolderForeground": "#9ba0a6",
		"statusBar.debuggingBackground": "#2a2f34",
		"statusBar.debuggingForeground": "#9ba0a6",
		"panel.border": "#2a2f34",
		"panelTitle.activeBorder": "#0f77cd"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment, punctuation.comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#60676e"
			}
		},
		{
			"name": "Operator",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation, delimiter, bracket, brace, paren, delimiter.tag, punctuation.tag, tag.html, tag.xml, meta.property-value punctuation.separator.key-value,punctuation.definition.metadata.md, string.link.md, meta.brace",
			"settings": {
				"foreground": "#7d848a"
			}
		},
		{
			"name": "String",
			"scope": "string, meta.property-value.string, support.constant.property-value.string, meta.structure.dictionary.value.json string.quoted.double.json, meta.structure.dictionary.json string.quoted.double.json, meta.preprocessor string",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7d848a"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": "constant.numeric, meta.property-value.numeric, support.constant.property-value.numeric, meta.property-value.color, support.constant.property-value.color,constant.language",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "User names",
			"scope": "constant.character, constant.other,entity.name.function, entity.name.class, entity.other.inherited-class, entity.other.attribute-name, entity.name, entity.other.attribute-name, entity.other.attribute-name.html, support.type.property-name, entity.name.tag.table, meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, meta.property-value.keyword, support.constant.property-value.keyword, meta.preprocessor.keyword,keyword.other.use, keyword.other.function.use, keyword.other.namespace, keyword.other.new, keyword.other.special-method, keyword.other.unit, keyword.other.use-as",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Storage",
			"scope": "storage, storage.type, storage.type.ts, storage.type.var.ts, storage.type.js, storage.type.var.js, storage.type.const.ts, storage.type.let.ts, storage.type.let.js, storage.type.const.js, entity.name.tag",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": "meta.ptr, meta.pointer, meta.address, meta.array.cxx",
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Library",
			"scope": "support.type, support.class, support.function, support.constant",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#ae1616"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#ae1616"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": "punctuation.definition.heading.md, entity.name.type.md, beginning.punctuation",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Markdown titles",
			"scope": "markup.heading, entity.name.section",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": "markup.raw, markup.inline.raw, markup.fenced, markup.fenced_code",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7d848a"
			}
		},
		{
			"name": "Markdown link",
			"scope": "markup.link, string.other.link.title, string.other.link.description, meta.link.inline, meta.image.inline",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": "variable.language.makefile, variable.other.makefile",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#d6dade"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": "entity.name.tag.css",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d6dade"
			}
		},
		{
			"name": "CSS Property",
			"scope": "meta.property-name.css",
			"settings": {
				"foreground": "#d6dade"
			}
		}
	]
}